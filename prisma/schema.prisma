datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Calendario {
  id    Int     @id @default(autoincrement())
  dia   String
  tarefas Tarefa[]
  @@map("calendario")
}

model Tarefa {
  id      Int     @id @default(autoincrement())
  tarefa  String
  data_id Int 
  status  String
  calendario Calendario @relation(fields: [data_id], references: [id])
  @@map("tarefa")
}


